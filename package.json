{
  "name": "ZeratorLive",
  "version": "1.2.0",
  "description": "Zerator Live Notifications",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist" : "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "ZeratorLive",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^16.0.8",
    "electron-builder": "^22.10.5"
  },
  "build" : {
    "appId" : "ZeratorLive",
    "productName" : "ZeratorLive",
    "target" : "NSIS",
    "nsis" : {
      "allowToChangeInstallationDirectory" : true,
      "oneClick" : false,
      "perMachine": true,
      "createStartMenuShortcut": true
    },
    "win" : {
      "icon" : "src/build.ico"
    }
  }
}
